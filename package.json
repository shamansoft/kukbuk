{
  "name": "mykukbuk-extension",
  "version": "0.3.9",
  "description": "Chrome extension to save recipes to Google Drive",
  "scripts": {
    "build": "node scripts/increment-version.js && node scripts/substitute-env.js && webpack --config webpack.config.js",
    "build:local": "node scripts/increment-version.js && node scripts/substitute-env.js --env=.env.local && cross-env NODE_ENV=development webpack --config webpack.config.js",
    "dev": "cross-env NODE_ENV=development node scripts/substitute-env.js && webpack --watch --config webpack.config.js",
    "dev:local": "cross-env NODE_ENV=development node scripts/substitute-env.js --env=.env.local && webpack --watch --config webpack.config.js",
    "zip": "npm run build && node scripts/zip-extension.js",
    "zip:local": "npm run build:local && node scripts/zip-extension.js",
    "deploy": "node scripts/deploy-chrome.js",
    "deploy:local": "node scripts/deploy-chrome.js --env=.env.local",
    "lint": "eslint --ext .js .",
    "lint:fix": "eslint --ext .js --fix .",
    "format": "prettier --write \"**/*.{js,html,css,json}\"",
    "test": "jest --config jest.config.js",
    "test:background": "jest --config jest.config.js background/background.test.js",
    "test:auth": "jest --config jest.config.js background/services/auth.test.js",
    "test:transform": "jest --config jest.config.js background/services/transformation.test.js",
    "serve:auth": "node scripts/serve-auth.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "firebase": "^10.14.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-transform-modules-commonjs": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@eslint/js": "^9.27.0",
    "adm-zip": "^0.5.9",
    "babel-jest": "^29.5.0",
    "babel-loader": "^10.0.0",
    "child_process": "^1.0.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "fs": "^0.0.1-security",
    "jest": "^29.6.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^22.1.0",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "prettier": "^3.5.3",
    "vitest": "^0.34.6",
    "webpack": "^5.76.0",
    "webpack-cli": "^5.0.1"
  }
}
